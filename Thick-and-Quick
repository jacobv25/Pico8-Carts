pico-8 cartridge // http://www.pico-8.com
version 41
__lua__

player = {
    spr = 1,
    spr2 = 3,
    x = 64,
    y = 64,
    top = 1,
    bot = 7,
    left = 2,
    right = 5
}
enemy = {
    spr = 17,
    x = 64,
    y = 64,
    top = 2,
    bot = 5,
    left = 0,
    right = 7
}
chaser = {
    spr = 36, -- change this to the sprite number you want for the Chaser
    x = 64,
    y = 64,
    top = 0,
    bot = 7,
    left = 0,
    right = 7,
    speed = 1 -- you can adjust this to change how fast the chaser moves towards the player
}

big_chaser = {
    sprs = {38, 39, 54, 55}, -- these are the sprite numbers for the four parts
    x = 64,
    y = 64,
    speed = .5, -- adjust this to change how fast the big chaser moves
    top = 0, -- adjust these as necessary
    bot = 15,
    left = 0,
    right = 15,

    tl_corner = { left = 0, right = 1, top = 0, bot = 1 }, --corners of the big chaser
    tr_corner = { left = 14, right = 15, top = 0, bot = 1 },
    bl_corner = { left = 0, right = 1, top = 14, bot = 15 },
    br_corner = { left = 14, right = 15, top = 14, bot = 15 },

    v_non_corner = { left = 2, right = 13, top = 0, bot = 0 }, --vertical parts of the big chaser that are not corners
    h_non_corner = { left = 0, right = 0, top = 2, bot = 13 }, --horizontal parts of the big chaser that are not corners
}

projectile = {
    spr = 20,
    spr2 = 36,
    x = 0,
    y = 0,
    top = 0,
    bot = 7,
    left = 0,
    right = 7
}

enemies = {}
projectiles = {}
last_enemy_projectile_time = {}
last_projectile_time = 0
last_enemy_time = 0
debug = {}
key_pressed = ""

chasers_on = false
shooters_on = false
projectiles_on = false
pxl_mvmt_on = true

-- Define map
current_room_x = 0
current_room_y = 0

-- Debug Player Position
playerTop = player.y + player.top
playerBot = player.y + player.bot
playerLeft = player.x + player.left
playerRight = player.x + player.right

-- -- Debug Enemy Position
-- enemyTop = enemy.y + enemy.top
-- enemyBot = enemy.y + enemy.bot
-- enemyLeft = enemy.x + enemy.left
-- enemyRight = enemy.x + enemy.right

-- -- Debug Projectile Position
-- projectileTop = projectile.y + projectile.top
-- projectileBot = projectile.y + projectile.bot
-- projectileLeft = projectile.x + projectile.left
-- projectileRight = projectile.x + projectile.right

-- Debug Big Chaser Position
bigChaserTop = big_chaser.y + big_chaser.top
bigChaserBot = big_chaser.y + big_chaser.bot
bigChaserLeft = big_chaser.x + big_chaser.left
bigChaserRight = big_chaser.x + big_chaser.right

function _init()
    cls()

    -- Enables mouse and keyboard support
    poke(0x5f2d, 1)

end

-- Game loop
function _update()

    update_player()

    update_enemies()

    update_big_chaser()

    update_projectiles()

    create_enemies()

    debug[1] = "player top: " .. playerTop
    debug[2] = "player bot: " .. playerBot
    debug[3] = "player left: " .. playerLeft
    debug[4] = "player right: " .. playerRight

    debug[5] = "big_c top: " .. bigChaserTop
    debug[6] = "big_c bot: " .. bigChaserBot
    debug[7] = "big_c left: " .. bigChaserLeft
    debug[8] = "big_c right: " .. bigChaserRight


end

function update_big_chaser()
    -- If big chaser is on, move it towards the player
    if big_chaser.speed then
        -- local dx = player.x - big_chaser.x
        -- local dy = player.y - big_chaser.y
        -- local len = sqrt(dx * dx + dy * dy)
        -- big_chaser.x = big_chaser.x + dx / len * big_chaser.speed
        -- big_chaser.y = big_chaser.y + dy / len * big_chaser.speed

        if check_big_chaser_collision(big_chaser, player) then
            -- If the player collides with the big chaser, print to screen
            debug[0] = "You were eaten by the big chaser!"
        else
            debug[0] = ""
        end
    end
    bigChaserTop = big_chaser.y + big_chaser.top
    bigChaserBot = big_chaser.y + big_chaser.bot
    bigChaserLeft = big_chaser.x + big_chaser.left
    bigChaserRight = big_chaser.x + big_chaser.right
end

function update_enemies()
    for i, enemy in pairs(enemies) do
        -- If enemy is a chaser, move it towards the player
        if enemy.speed then
            local dx = player.x - enemy.x
            local dy = player.y - enemy.y
            local len = sqrt(dx * dx + dy * dy)
            enemy.x = enemy.x + dx / len * enemy.speed
            enemy.y = enemy.y + dy / len * enemy.speed
        end

        -- Initialize last projectile time for this enemy if not already done
        if not last_enemy_projectile_time[i] then
            last_enemy_projectile_time[i] = time()
        end

        if projectiles_on then 
            -- If 3 seconds have passed, spawn a projectile
            if time() - last_enemy_projectile_time[i] >= 3 then
                spawn_projectile(enemy)
                last_enemy_projectile_time[i] = time()
            end
        end
    end
end

function create_enemies()
    if(chasers_on or shooters_on) then
        if time() - last_enemy_time >= 3 then
            spawn_enemy()
            last_enemy_time = time()
        end
    end
end

function _draw()
    cls()

    draw_map()
    draw_player()
    draw_enemies()
    draw_projectiles()

    draw_big_chaser(big_chaser)

    -- if debug then
        -- If debug is true, draw debug information
        draw_debug()
        -- draw_mouse()
    -- end
end

function draw_player()
    -- Draw player
    spr(player.spr, player.x, player.y)
end

function draw_enemies()
    for e in all(enemies) do
        spr(e.spr, e.x, e.y)
    end
end

function draw_big_chaser(chaser)
    spr(chaser.sprs[1], chaser.x, chaser.y)
    spr(chaser.sprs[2], chaser.x + 8, chaser.y)
    spr(chaser.sprs[3], chaser.x, chaser.y + 8)
    spr(chaser.sprs[4], chaser.x + 8, chaser.y + 8)
end

-- Function to draw projectiles
function draw_projectiles()
    for proj in all(projectiles) do
        spr(proj.spr, proj.x, proj.y)
    end
end

function draw_map()
    map(current_room_x, current_room_y)
end

function draw_mouse()
    -- Draw sprite at mouse coordinates. The -1 is to adjust for PICO-8's 1-indexed nature.
    spr(0, stat(32) - 1, stat(33) - 1)

    -- Print the state of the mouse buttons to the console
    print(stat(34))
end

function check_big_chaser_collision(chaser, player)
    -- Check collision with vertical non-corner parts of the big chaser
    local verticalCollision = check_collision(player, {
        x = chaser.x + chaser.v_non_corner.left,
        y = chaser.y + chaser.top,
        left = 0,
        right = chaser.v_non_corner.right - chaser.v_non_corner.left,
        top = 0,
        bot = chaser.bot - chaser.top
    })

    -- Check collision with horizontal non-corner parts of the big chaser
    local horizontalCollision = check_collision(player, {
        x = chaser.x + chaser.left,
        y = chaser.y + chaser.h_non_corner.top,
        left = 0,
        right = chaser.right - chaser.left,
        top = 0,
        bot = chaser.h_non_corner.bot - chaser.h_non_corner.top
    })

    -- Check if player is in any of the corner areas
    local inCorner = false
    local corners = {chaser.tl_corner, chaser.tr_corner, chaser.bl_corner, chaser.br_corner}
    for _, corner in pairs(corners) do
        if check_collision(player, {
            x = chaser.x + corner.left,
            y = chaser.y + corner.top,
            left = 0,
            right = corner.right - corner.left,
            top = 0,
            bot = corner.bot - corner.top
        }) then
            inCorner = true
            break
        end
    end

    -- If the player is in a corner but not in a non-corner part, return false
    if inCorner and not (verticalCollision or horizontalCollision) then
        return false
    end

    -- Return true if there's a collision with any non-corner part
    return verticalCollision or horizontalCollision
end

-- This function checks if two rectangles overlap (collide)
function check_collision(a, b)
    local a_left = a.x + a.left
    local a_right = a.x + a.right
    local a_top = a.y + a.top
    local a_bottom = a.y + a.bot

    local b_left = b.x + b.left
    local b_right = b.x + b.right
    local b_top = b.y + b.top
    local b_bottom = b.y + b.bot

    -- Check if a is to the right of b
    if a_left > b_right then
        return false
    end
    -- Check if a is to the left of b
    if a_right < b_left then
        return false
    end
    -- Check if a is below b
    if a_top > b_bottom then
        return false
    end
    -- Check if a is above b
    if a_bottom < b_top then
        return false
    end

    -- If none of the above conditions are true, rectangles must be colliding
    return true
end

-- Function to spawn projectiles from a given enemy
function spawn_projectile(enemy)
    local proj = {
        spr = projectile.spr,
        x = enemy.x,
        y = enemy.y,
        vx = (player.x - enemy.x) / 30, -- Simple targeting, adjust the number for speed
        vy = (player.y - enemy.y) / 30,
        top = projectile.top,
        bot = projectile.bot,
        left = projectile.left,
        right = projectile.right
    }
    add(projectiles, proj)

    sfx(0)
end

-- Function to update projectiles
function update_projectiles()
    for proj in all(projectiles) do
        proj.x = proj.x + proj.vx
        proj.y = proj.y + proj.vy

        -- Check if projectile hits the player
        if check_collision(player, proj) then
            -- Handle collision with player
            -- for now, we just remove the projectile
            del(projectiles, proj)
        end
    end
end

-- Function to spawn enemies
function spawn_enemy()
    if chasers_on and shooters_on then
        -- If both chasers and shooters are on, pick a random enemy type
        local type = flr(rnd(2))
    end
    if chasers_on and not shooters_on then
        -- If only chasers are on, spawn a chaser
        local type = 1
    end
    if not chasers_on and shooters_on then
        -- If only shooters are on, spawn a shooter
        local type = 0
    end

    -- Randomly pick an enemy type
    local new_enemy
    if type == 0 then
        new_enemy = {
            spr = enemy.spr,
            x = rnd(128), -- Random x position on the screen
            y = rnd(128), -- Random y position on the screen
            top = enemy.top,
            bot = enemy.bot,
            left = enemy.left,
            right = enemy.right
        }
    else
        new_enemy = {
            spr = chaser.spr,
            x = rnd(128), -- Random x position on the screen
            y = rnd(128), -- Random y position on the screen
            top = chaser.top,
            bot = chaser.bot,
            left = chaser.left,
            right = chaser.right,
            speed = chaser.speed
        }
    end
    add(enemies, new_enemy)
end

function test_collision(p, e)
    if check_collision(p, e) then
        -- If there's a collision, use certain sprites
        spr(p.spr2, p.x, p.y)
        spr(e.spr2, e.x, e.y)
    else
        -- If there's no collision, use other sprites
        spr(p.spr, p.x, p.y)
        spr(e.spr, e.x, e.y)
    end
end

-- Function to move player with arrow keys
function update_player()
    if pxl_mvmt_on then
        if btnp(0) then player.x = player.x - 1 end
        -- Left
        if btnp(1) then player.x = player.x + 1 end
        -- Right
        if btnp(2) then player.y = player.y - 1 end
        -- Up
        if btnp(3) then player.y = player.y + 1 end
        -- Down
    else
        if btn(0) then player.x = player.x - 1 end
        -- Left
        if btn(1) then player.x = player.x + 1 end
        -- Right
        if btn(2) then player.y = player.y - 1 end
        -- Up
        if btn(3) then player.y = player.y + 1 end
        -- Down
    end

    playerTop = player.y + player.top
    playerBot = player.y + player.bot
    playerLeft = player.x + player.left
    playerRight = player.x + player.right
end

-- Function to draw debug information
function draw_debug()
    for line_number, text in pairs(debug) do
        local y_coordinate = line_number * 6 -- Each line is 6 pixels apart
        print(text, 0, y_coordinate, 7)
    end
end

