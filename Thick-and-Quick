pico-8 cartridge // http://www.pico-8.com
version 41
__lua__

player = {
    spr = 1,
    spr2 = 3,
    x = 10,
    y = 10,
    top = 1,
    bot = 7,
    left = 2,
    right = 5
}
enemy = {
    spr = 17,
    x = 64,
    y = 64,
    top = 2,
    bot = 5,
    left = 0,
    right = 7
}

projectile = {
    spr = 20,
    spr2 = 36,
    x = 0,
    y = 0,
    top = 0,
    bot = 7,
    left = 0,
    right = 7
}

enemies = {}
projectiles = {}
last_enemy_projectile_time = {}
last_projectile_time = 0
last_enemy_time = 0

debug = false
-- Define map
current_room_x = 0
current_room_y = 0

playerTop = player.y + player.top
playerBot = player.y + player.bot
playerLeft = player.x + player.left
playerRight = player.x + player.right

enemyTop = enemy.y + enemy.top
enemyBot = enemy.y + enemy.bot
enemyLeft = enemy.x + enemy.left
enemyRight = enemy.x + enemy.right

projectileTop = projectile.y + projectile.top
projectileBot = projectile.y + projectile.bot
projectileLeft = projectile.x + projectile.left
projectileRight = projectile.x + projectile.right




-- Game loop
function _update()
    -- Debug mode toggle
    if btnp(5) then
        debug = not debug -- toggle debug mode
    end

    update_player()

    for i, enemy in pairs(enemies) do
        -- Initialize last projectile time for this enemy if not already done
        if not last_enemy_projectile_time[i] then
            last_enemy_projectile_time[i] = time()
        end

        -- If 3 seconds have passed, spawn a projectile
        if time() - last_enemy_projectile_time[i] >= 3 then
            spawn_projectile(enemy)
            last_enemy_projectile_time[i] = time()
        end
    end

    if time() - last_enemy_time >= 3 then -- Spawn an enemy every 3 seconds
        spawn_enemy()
        last_enemy_time = time()
    end
    update_projectiles()
end


function _draw()
    cls()

    draw_map()

    draw_player()
    draw_enemies()
    draw_projectiles()
    
    if debug then
        -- If debug is true, draw debug information
        draw_debug()
    end
end

function draw_player()
    -- Draw player
    spr(player.spr, player.x, player.y)
end

function draw_enemies()
    for e in all(enemies) do
        spr(e.spr, e.x, e.y)
    end
end

-- Function to draw projectiles
function draw_projectiles()
    for proj in all(projectiles) do
        spr(proj.spr, proj.x, proj.y)
    end
end

function draw_map()
    map(current_room_x, current_room_y)
end

-- This function checks if two rectangles overlap (collide)
function check_collision(a, b)
    local a_left = a.x + a.left
    local a_right = a.x + a.right
    local a_top = a.y + a.top
    local a_bottom = a.y + a.bot

    local b_left = b.x + b.left
    local b_right = b.x + b.right
    local b_top = b.y + b.top
    local b_bottom = b.y + b.bot

    -- Check if a is to the right of b
    if a_left > b_right then
        return false
    end
    -- Check if a is to the left of b
    if a_right < b_left then
        return false
    end
    -- Check if a is below b
    if a_top > b_bottom then
        return false
    end
    -- Check if a is above b
    if a_bottom < b_top then
        return false
    end

    -- If none of the above conditions are true, rectangles must be colliding
    return true
end

-- Function to spawn projectiles from a given enemy
function spawn_projectile(enemy)
    local proj = {
        spr = projectile.spr,
        x = enemy.x,
        y = enemy.y,
        vx = (player.x - enemy.x) / 30, -- Simple targeting, adjust the number for speed
        vy = (player.y - enemy.y) / 30,
        top = projectile.top,
        bot = projectile.bot,
        left = projectile.left,
        right = projectile.right
    }
    add(projectiles, proj)
end


-- Function to update projectiles
function update_projectiles()
    for proj in all(projectiles) do
        proj.x = proj.x + proj.vx
        proj.y = proj.y + proj.vy

        -- Check if projectile hits the player
        if check_collision(player, proj) then
            -- Handle collision with player
            -- for now, we just remove the projectile
            del(projectiles, proj)
        end
    end
end

function spawn_enemy()
    local newEnemy = {
        spr = enemy.spr,
        spr2 = enemy.spr2,
        x = rnd(128), -- Random x position on screen
        y = rnd(128), -- Random y position on screen
        w = enemy.w, 
        h = enemy.h 
    }
    add(enemies, newEnemy)
end

function test_collision(p, e)
    if check_collision(p, e) then
        -- If there's a collision, use certain sprites
        spr(p.spr2, p.x, p.y)
        spr(e.spr2, e.x, e.y)
    else
        -- If there's no collision, use other sprites
        spr(p.spr, p.x, p.y)
        spr(e.spr, e.x, e.y)
    end
end

-- Function to move player with arrow keys
function update_player()
    if debug then
        if btnp(0) then player.x = player.x - 1 end
        -- Left
        if btnp(1) then player.x = player.x + 1 end
        -- Right
        if btnp(2) then player.y = player.y - 1 end
        -- Up
        if btnp(3) then player.y = player.y + 1 end
        -- Down
    else
        if btn(0) then player.x = player.x - 1 end
        -- Left
        if btn(1) then player.x = player.x + 1 end
        -- Right
        if btn(2) then player.y = player.y - 1 end
        -- Up
        if btn(3) then player.y = player.y + 1 end
        -- Down
    end

    playerTop = player.y + player.top
    playerBot = player.y + player.bot
    playerLeft = player.x + player.left
    playerRight = player.x + player.right
end

function draw_debug(a, b)
    -- print("fps: " .. flr(stat(7)), 0, 6, 7)

    -- print player's top, bottom, left, and right
    print("player top: " .. playerTop, 0, 12, 7)
    print("player bot: " .. playerBot, 0, 18, 7)
    print("player left: " .. playerLeft, 0, 24, 7)
    print("player right: " .. playerRight, 0, 30, 7)
    
    print("--------------------", 0, 36, 7)

    -- print enemy's top, bottom, left, and right
    -- print("enemy top: " .. enemyTop, 0, 42, 7)
    -- print("enemy bot: " .. enemyBot, 0, 48, 7)
    -- print("enemy left: " .. enemyLeft, 0, 54, 7)
    -- print("enemy right: " .. enemyRight, 0, 60, 7)

    -- print projectile's top, bottom, left, and right
    print("projectile top: " .. projectileTop, 0, 42, 7)
    print("projectile bot: " .. projectileBot, 0, 48, 7)
    print("projectile left: " .. projectileLeft, 0, 54, 7)
    print("projectile right: " .. projectileRight, 0, 60, 7)

end

